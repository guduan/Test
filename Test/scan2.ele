&run_setup
        lattice = fodo.lte,
        use_beamline = bl,
        final = %s.fin,
        centroid = scan2-<seed>.cen,
        p_central = 880,
		magnets=%s.mag
		always_change_p0 =1
		random_number_seed="( <randomNumber> 2 +)"
		parameters = %s.param
&end

&load_parameters
	filename = scan1-<loadnumber>.param,
	change_defined_values = 1
	force_occurence_data = 1
&end

&run_control
        n_indices = 1,
        reset_rf_for_each_step = 0,
		first_is_fiducial= 1
&end

&vary_element 
    name=malin, 
    item=dp, 
	index_number=0,
	index_limit=9,
	enumeration_file = scan.sdds, 
	enumeration_column = dp
&end

&twiss_output
    filename = %s.twi
    matched = 0,  
    output_at_each_step=0, 
	beta_x = 16.3093  
	beta_y = 4.06314
&end

&matrix_output
    SDDS_output = %s.mat
    SDDS_output_order = 2
&end

&bunched_beam
        n_particles_per_bunch = 1
&end

&track &end
